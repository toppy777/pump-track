{
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "objectWrap": "preserve",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "jsxBracketSameLine:": false,
  "plugins": ["prettier-plugin-organize-imports"]
}
